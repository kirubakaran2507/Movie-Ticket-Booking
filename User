class User {
    private int id;
    private String name;
    private String emailId;
    private long phoneNumber;

    public User(int id, String name, String emailId, long phoneNumber) {
        this.id = id;
        this.name = name;
        this.emailId = emailId;
        this.phoneNumber = phoneNumber;
    }

    public int getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public String getEmailId() {
        return emailId;
    }

    public long getPhoneNumber() {
        return phoneNumber;
    }
}
